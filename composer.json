{
    "name": "your-vendor/laravel-cloud-connector",
    "description": "A Laravel package for interacting with the Laravel Cloud API",
    "keywords": ["laravel", "laravel-cloud", "api", "cloud", "deployment"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelCloudConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCloudConnector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCloudConnector\\LaravelCloudServiceProvider"
            ],
            "aliases": {
                "LaravelCloud": "LaravelCloudConnector\\Facades\\LaravelCloud"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
